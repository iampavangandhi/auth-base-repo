{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "rules": {
    "no-underscore-dangle": 0,
    "object-curly-newline": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-shadow": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "quotes": [2, "double", "avoid-escape"]
  },
  "env": {
    "jest": true
  }
}
